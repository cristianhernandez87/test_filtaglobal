    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^2.1.0",
    "eslint": "^5.16.0",
    "file-loader": "^3.0.1",
    "handlebars-loader": "^1.6.0",
    "html-loader": "^0.5.1",
    "mini-css-extract-plugin": "^0.5.0",
